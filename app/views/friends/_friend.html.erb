
<div id="<%= dom_id friend %>">
<table class="table table-stripped">
  <thead>
    <tr>
    <th>First name:</th>
    <th>Last name:</th>
    <th>Email:</th>
    <th>Phone:</th>
    <th>Facebook:</th>
    <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
  
      <tr>
        <td><%= friend.first_name %></td>    
        <td><%= friend.last_name %></td>
        <td><%= friend.email %></td>
        <td><%= friend.phone %>  </td>
        <td><%= friend.facebook %></td>
        <td colspan="3"></td>
      </tr>
    </tbody>
  </table>  
</div>